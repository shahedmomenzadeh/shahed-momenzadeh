#include <stdio.h>
int main()
{
        float n;
        printf("Enter a Number : ");
        scanf("%f" , &n);
        if(n>0)
        {
            float i;
            for(i=0.0001 ; i<n ; i+=0.0001)
                {
                    if(i*i*i > n-0.0001)
                    {
                        printf("The Third Root Of %.3f = %.3f" , n , i);
                        break;
                    }
                }
        }
        else if(n<0)
        {
            n = -n;
            float i;
            for(i=0.0001 ; i<n ; i+=0.0001)
                {
                    if(i*i*i > n-0.0001)
                    {
                        printf("The Third Root Of %.3f = %.3f" , -n , -i);
                        break;
                    }
                }
        }
        else if(n==0)
        {
            printf("The third root of 0 = 0");
        }
    return 0;
}
